# Alumínio
Regra_Alumínio: 
    Material(?m), 
    espessuraMin(?m, ?emin), 
    espessuraMax(?m, ?emax), 
    potenciaLaserKW(?m, ?p), 
    toleranciaMM(?m, ?t),
    gasAuxiliar(?m, ?g), 
    qualidadeCorte(?m, ?q), 
    swrlb:greaterThanOrEqual(?esp, ?emin), 
    swrlb:lessThanOrEqual(?esp, ?emax), 
    swrlb:greaterThanOrEqual(?pot, ?p), 
    swrlb:lessThanOrEqual(?tol, ?t) 
    -> CorteLaser(?c), temMaterial(?c, ?m) .

